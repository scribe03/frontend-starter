{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@env/*": [
        "src/environments/*"
      ],
      "@env-app-concept/*": [
        "projects/app-concept/src/environments/*"
      ],
      "@env-app-science-deparment/*": [
        "projects/app-science-department/src/environments/*"
      ],
      "@env-app-transport-deparment/*": [
        "projects/app-transport-department/src/environments/*"
      ],
      "@master/*": [
        "src/app/*"
      ],
      "@scribe": [
        "dist/lib-scribe"
      ],
      "@scribe/*": [
        "dist/lib-scribe/*"
      ],
      "@scribe-ui": [
        "dist/lib-scribe-ui"
      ],
      "@scribe-ui/*": [
        "dist/lib-scribe-ui/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
